#include <stdio.h>
int main()
{
	int a, b, c, t;
	scanf("%d %d %d", &a, &b, &c);
	if (a > b) {
		t = a;
		a = b;
		b = t;
	}
	if (a > c) {
		t = a;
		a = c;
		c = t;
	}
	if (b > c) {
		t = b;
		b = c;
		c = t;
	}
    int n, i;
	if (a > c)
		n = c;
	else
		n = a;
	for (i = n; i >1; i--)
	{
		if (a % i == 0 && c % i == 0)
		{
			a /= i;
			c /= i;
		}
 
	}
	printf("%d/%d", a, c);

	return 0;
}


